<h1>Tracking prospects from the following teams</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Team</th>
      <th># of players tracked</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |team|}}
      <tr>
        <td>{{#link-to "teams.show" team.code}}{{team.name}} ({{team.code}}){{/link-to}}</td>
        <td>{{team.playerCount}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{outlet}}
